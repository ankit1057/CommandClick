<!--                        android:layout_weight="0.1"/>-->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:id="@+id/cmdindex_setting_button"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/toolbar_layout_height"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="vertical"
                        android:foreground="?selectableItemBackground"
                        android:layout_weight="0.2"
                        >
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/cmdindex_setting_button_image"
                            android:layout_width="@dimen/toolbar_image_width"
                            android:layout_height="@dimen/toolbar_image_height"
                            android:src="@drawable/extra"
                            android:backgroundTint="@color/white"
                            app:tint="@color/file_dark_green_color"
                            android:scaleType="fitCenter"
                            android:layout_gravity="center"
                            android:padding="0dp"
                            />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/cmdindex_setting_button_caption"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:scaleType="fitCenter"
                            android:padding="0dp"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:ellipsize="end"
                            android:maxLines="1"
                            android:layout_marginStart="4dp"
                            android:layout_marginEnd="4dp"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="extra"
                            />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
<!--            </android.widget.Toolbar>-->
        </androidx.appcompat.widget.LinearLayoutCompat>

