<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <data>

    </data>
    <android.widget.Toolbar
        android:id="@+id/web_search_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentInsetEnd="0dp"
        android:contentInsetStart="0dp"
        android:focusableInTouchMode="true"
        android:backgroundTint="@color/white"
        >
        <LinearLayout
            android:id="@+id/web_search_toolbar_linear_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="1">
            <AutoCompleteTextView
                android:id="@+id/cmd_web_search_edit_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:hint="@string/web_search"
                android:text=""
                android:textColorHighlight ="@color/edit_text_selected_color"
                android:textCursorDrawable="@null"
                android:autofillHints=""
                android:selectAllOnFocus="true"
                android:layout_weight="0.8"
                android:background="@null"
                android:layout_gravity="center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:maxLines="1"
                android:inputType="text" />
            <ImageButton
                android:id="@+id/web_search_cancel"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@drawable/icons8_close"
                android:backgroundTint="@color/white"
                android:layout_gravity="center"
                android:contentDescription="@string/setting"
                android:layout_weight="0.2"/>
        </LinearLayout>
    </android.widget.Toolbar>
</layout>
