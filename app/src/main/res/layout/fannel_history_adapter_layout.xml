    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/fannel_history_adapter_pin"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:src="@drawable/pin"
        app:tint="@color/file_dark_green_color"
        android:backgroundTint="@color/setting_menu_footer"
        android:layout_gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/fannel_history_adapter_relative_layout"
        app:layout_constraintStart_toEndOf="@+id/fannel_history_adapter_logo_relative_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/fannel_history_adapter_edit"
        app:layout_constraintHorizontal_weight="1"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        />
    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/fannel_history_adapter_edit"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:src="@drawable/icons8_edit"
        app:tint="@color/gray_out"
        android:backgroundTint="@color/setting_menu_footer"
        android:layout_gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/fannel_history_adapter_logo_relative_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fannel_history_adapter_long_press"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="1"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        />
    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/fannel_history_adapter_long_press"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:src="@drawable/long_press"
        app:tint="@color/gray_out"
        android:backgroundTint="@color/setting_menu_footer"
        android:layout_gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/fannel_history_adapter_logo_relative_layout"
        app:layout_constraintStart_toEndOf="@+id/fannel_history_adapter_edit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        />
