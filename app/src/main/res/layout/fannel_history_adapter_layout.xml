<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fannel_history_adapter_constraint_layout"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginStart="0.5dp"
    android:layout_marginEnd="0.5dp"
    android:layout_marginTop="1dp"
    android:layout_width="match_parent"
    android:paddingTop="10dp"
    android:background="@color/setting_menu_footer"
    >
    <RelativeLayout
        android:id="@+id/fannel_history_adapter_relative_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="0dp"
        android:layout_margin="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/fannel_history_adapter_logo_relative_layout"
        app:layout_constraintDimensionRatio="h, 1:1.4"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/fannel_history_adapter_capture"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:adjustViewBounds="true"
            />
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_gravity="center_vertical"
            android:layout_margin="10dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            >
<!--             android:layout_centerInParent="true"
            android:layout_centerVertical="true"-->
            <com.puutaro.commandclick.custom_view.OutlineTextView
                android:id="@+id/fannel_history_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:textSize="16sp"
                android:textStyle="bold"
                />
            <com.puutaro.commandclick.custom_view.OutlineTextView
                android:id="@+id/fannel_history_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="22sp"
                android:textStyle="bold"
                android:gravity="center"
                android:ellipsize="marquee"
                android:layout_marginBottom="0dp"
                />
        </androidx.appcompat.widget.LinearLayoutCompat>
    </RelativeLayout>

<!--    <androidx.appcompat.widget.LinearLayoutCompat-->
<!--        android:id="@+id/fannel_history_adapter_bottom_linear_inner"-->
<!--        android:orientation="horizontal"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="30dp"-->
<!--        android:background="@color/setting_menu_footer"-->
<!--        app:tint="@color/white"-->
<!--        android:weightSum="1"-->
<!--        >-->
    <RelativeLayout
        android:id="@+id/fannel_history_adapter_logo_relative_layout"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/fannel_history_adapter_relative_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fannel_history_adapter_pin"
        app:layout_constraintBottom_toTopOf="@+id/fannel_history_adapter_edit"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="1"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        >
        <!--
        android:layout_weight="0.3333"-->
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/fannel_history_adapter_icon"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/icons8_share"
            android:backgroundTint="@color/setting_menu_footer"
            app:tint="@color/file_dark_green_color"
            android:scaleType="fitXY"
            android:padding="0dp"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
<!--    <androidx.appcompat.widget.AppCompatImageButton-->
<!--        android:id="@+id/fannel_history_adapter_icon"-->
<!--        android:layout_width="0dp"-->
<!--        android:layout_height="30dp"-->
