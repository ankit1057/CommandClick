rootfs_dir="/data/data/com.puutaro.commandclick/files/1/rootfs/"
cd "${files_dir}"
echo --- pwd
pwd
echo --- ls
ls
tar -xzvf rootfs.tar.gz \
--exclude sys \
--exclude dev \
--exclude proc \
--exclude mnt \
--exclude support \
-C "${rootfs_dir}" \
>/dev/null
exit 0
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
PROOT_TMP_DIR=$ROOTFS_PATH/support/ \
PROOT_LOADER=$LIB_PATH/loader \
PROOT_LOADER_32=$LIB_PATH/loader32 \
